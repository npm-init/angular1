{
  "prompts": {
    "name": {
      "type": "string",
      "required": true,
      "message": "Project Name"
    },
    "description": {
      "type": "input",
      "required": true,
      "message": "Project Description"
    }
  },
  "compile" : {
    "with": "mustache",
    "excludes": ["\.html$"]
  },
  "completeMessage": "To get started:\n  cd {{name}}\n  npm install\n  npm start"
}
